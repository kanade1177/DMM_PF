<h2>User info</h2>
 <div class="profile-container">
   <%= attachment_image_tag user, :profile_image, :fill, 150, 150, format: "jpeg",class: "img-fluid rounded-circle" %>
   <table class="table ">
     <tbody>
       <tr></tr>
       <tr>
         <th>name</th>
         <th><%= user.name %></th>
       </tr>
       <tr>
         <th>introduction</th>
         <th><%= user.introduction %></th>
       </tr>
     </tbody>
   </table>
 </div>